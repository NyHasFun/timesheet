<h1>Single Shift</h1>
<form action="/shifts/edit/" method = "post">
  <input name="id" value="{{id}}" readonly><br>
  <input type="text" placeholder="Start Time" name = "startTime" value = "{{startTime}}"><br>
  <input type="text" placeholder="End Time" name = "endTime" value = "{{endTime}}"><br>
  <input type="text" placeholder="Productivity" name = "productivity" value = "{{productivity}}"><br>
  <input type="Submit">
</form>
